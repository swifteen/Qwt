####### Build rules

all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) /OUT:$(TARGET) @<<
	    $(OBJECTS) $(OBJMOC) $(LIBS)
<<

moc: $(SRCMOC)

tmake: Makefile

Makefile: dials.pro
	tmake dials.pro -o Makefile

dist:
	$(ZIP) dials.zip dials.pro $(SOURCES) $(HEADERS) $(DIST) $(INTERFACES)

