####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .c

.cpp.obj:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -Fo$@ $<

.cxx.obj:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -Fo$@ $<

.cc.obj:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -Fo$@ $<

.c.obj:
	$(CC) -c $(CFLAGS) $(INCPATH) -Fo$@ $<

####### Build rules

all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) /OUT:$(TARGET) @<<
	    $(OBJECTS) $(OBJMOC) $(LIBS)
<<

moc: $(SRCMOC)

tmake: Makefile

Makefile: radio.pro
	tmake radio.pro -o Makefile

dist:
	$(ZIP) radio.zip radio.pro $(SOURCES) $(HEADERS) $(DIST) $(INTERFACES)

clean:
	-del obj\radio.obj
	-del obj\ampfrm.obj
	-del obj\tunerfrm.obj
	-del moc\moc_ampfrm.cpp
	-del moc\moc_tunerfrm.cpp
	-del obj\moc_ampfrm.obj
	-del obj\moc_tunerfrm.obj
	-del $(TARGET)

