CFLAGS	=	-nologo -W3 -DQT_DLL -DQWT_DLL -DQT_THREAD_SUPPORT -DNO_DEBUG
CXXFLAGS=	-nologo -W3 -DQT_DLL -DQWT_DLL -DQT_THREAD_SUPPORT -DNO_DEBUG
INCPATH	=	-I"..\..\include" -I"$(QTDIR)\include"
LINK	=	link
LFLAGS	=	/NOLOGO /SUBSYSTEM:windows
LIBS	=	$(QTDIR)\lib\qt-mt230nc.lib $(QTDIR)\lib\qtmain.lib kernel32.lib user32.lib gdi32.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib imm32.lib winmm.lib wsock32.lib ..\..\lib\qwt.lib
